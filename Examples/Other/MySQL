CREATE TABLE currentPlayerInfo (
    GSID int PRIMARY KEY NOT null AUTO_INCREMENT,
    gameStoreName varchar(255),
	  playersTotal int,
    playersMagic int,
    playersPokemon int,
    playersYugioh int,
    playersDND int,
    playersWarhammer int,
    playersOther int
);

CREATE TABLE gameStoreInfo (
    GSID int PRIMARY KEY NOT null AUTO_INCREMENT,
    gameStoreName varchar(255),
    latitude double,
    longitude double,
    description text,
    street text,
    state text,
    city text,
    zip text,
    phone text,
    email text,
    logo text
);
